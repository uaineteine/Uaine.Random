<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.0</TargetFramework>
    <Authors>Daniel Stamer-Squair</Authors>
    <Company>Uaine Teine</Company>
    <RepositoryType>rng</RepositoryType>
    <PackageIcon>icon.png</PackageIcon>
    <PackageIconUrl />
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Copyright>Daniel Stamer-Squair 2022</Copyright>
    <Description>A System.Random extension class with additional functions</Description>
    <Version>1.1.2</Version>
    <PackageProjectUrl>https://github.com/uaineteine/Uaine.Random</PackageProjectUrl>
    <RepositoryUrl>https://github.com/uaineteine/Uaine.Random</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Uaine.Objects" Version="1.10.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
